{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "member-access": false,
    "jsx-self-close": false,
    "member-ordering": false,
    "ordered-imports": false,
    "no-debugger": false,
    "no-console": false,
    "no-empty-interface": false,
    "jsx-boolean-value": false,
    "prefer-const": false,
    "jsdoc-format": false,
    "interface-over-type-literal": false
  }
}